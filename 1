class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int n=nums.size();
        int sum=0;
        int sums=0;
        for(int i=0;i<n;i++){
        
            sums+=nums[i];
        }
         for(int i=1;i<=n;i++){
        
            sum+=i;
        }
        return sum-sums;
    }
};
